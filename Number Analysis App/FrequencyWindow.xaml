<Window x:Class="Number_Analyser_App.FrequencyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Number_Analyser_App"
        mc:Ignorable="d"
        Title="FrequencyWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dataGrid" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Button x:Name="button" Content="Close" HorizontalAlignment="Left" Margin="290,329,0,0" VerticalAlignment="Top" Click="button_Click"/>

    </Grid>
</Window>
